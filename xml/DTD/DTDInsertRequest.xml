<talnet type="request">
	<request type="DTD" name="INSERT">
		<description>
			DTD Insert
		</description>
		<requestArguments>
			<requestArgument type="String" name="tableName" key="into"></requestArgument>
			<requestArgument type="Map" name="data" key="data"></requestArgument>
		</requestArguments>
		<validation>
			<string target="{creds:appname}_{tableName}" type="isAlphaNumeric" errorid="15">
			</string>
			<foreach argname="data" errorid="15">
				<string target="{col}" type="isAlphaNumeric" errorid="15">
				</string>
			</foreach>
			<existence type="TableByName" errorid="12">
				<argument id="1">{creds:appname}_{tableName}</argument>
			</existence>
			<permission type="TablePermissionUser" errorid="6">
				<argument id="1">{creds:appname}_{tableName}</argument>
				<argument id="2">{creds:username}</argument>
				<argument id="3">INSERT</argument>
			</permission>
		</validation>
		<performance>
			<dynamicSqlExecute query="insert" return="return">
				<argument id="1">{creds:appname}_{tableName}</argument>
				<argument id="2">{data}</argument>
			</dynamicSqlExecute>
		</performance>
	</request>
</talnet>